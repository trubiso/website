generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model qa {
  created_at DateTime @id @db.Timestamptz(6)
  question   String
  answer     String?
}

model users {
  id       String @id @db.Uuid
  username String @unique
  password String
  token    String[]
}
